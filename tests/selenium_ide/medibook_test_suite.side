        {
          "id": "8",
          "comment": "Select time slot",
          "command": "click",
          "target": "css=input[name='appointment_time']:first-of-type",
          "targets": [
            ["css=input[name='appointment_time']:first-of-type", "css:finder"]
          ],
          "value": ""
        },
        {
          "id": "9",
          "comment": "Enter symptoms",
          "command": "type",
          "target": "name=symptoms",
          "targets": [
            ["name=symptoms", "name"]
          ],
          "value": "Regular checkup"
        },
        {
          "id": "10",
          "comment": "Submit booking",
          "command": "click",
          "target": "css=button[type='submit']",
          "targets": [
            ["css=button[type='submit']", "css:finder"]
          ],
          "value": ""
        },
        {
          "id": "11",
          "comment": "Verify booking success",
          "command": "waitForElementPresent",
          "target": "css=.alert-success, [href*='dashboard']",
          "targets": [],
          "value": "5000"
        }
      ]
    },
    {
      "id": "logout-test",
      "name": "Logout Test",
      "commands": [
        {
          "id": "1",
          "comment": "Assume user is logged in",
          "command": "open",
          "target": "/",
          "targets": [],
          "value": ""
        },
        {
          "id": "2",
          "comment": "Click user dropdown",
          "command": "click",
          "target": "id=navbarDropdown",
          "targets": [
            ["id=navbarDropdown", "id"]
          ],
          "value": ""
        },
        {
          "id": "3",
          "comment": "Click logout",
          "command": "click",
          "target": "linkText=Logout",
          "targets": [
            ["linkText=Logout", "linkText"]
          ],
          "value": ""
        },
        {
          "id": "4",
          "comment": "Verify logout success",
          "command": "waitForElementPresent",
          "target": "linkText=Login",
          "targets": [
            ["linkText=Login", "linkText"]
          ],
          "value": "5000"
        }
      ]
    }
  ],
  "suites": [
    {
      "id": "complete-user-flow",
      "name": "Complete User Flow",
      "persistSession": false,
      "parallel": false,
      "timeout": 300,
      "tests": [
        "patient-registration-test",
        "login-test",
        "doctor-search-test",
        "appointment-booking-test",
        "logout-test"
      ]
    }
  ],
  "urls": [
    "http://127.0.0.1:8000"
  ],
  "plugins": []
}
